{
  "presets": [ 
    [ "@babel/preset-env", {
      "targets": {
        "node": "current"
      },
      "useBuiltIns": "entry"
    } ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-classes",
    "@babel/plugin-transform-destructuring",
    [ "@babel/plugin-proposal-object-rest-spread", { "loose": true } ]
  ]
}
